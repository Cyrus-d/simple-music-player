<Project>
  <!-- Project properties -->
  <PropertyGroup>
    <TargetFrameworks>net472;net462</TargetFrameworks>
    <AppendTargetFrameworkToOutputPath>true</AppendTargetFrameworkToOutputPath>
    <GenerateDocumentationFile>false</GenerateDocumentationFile>
    <LangVersion>latest</LangVersion>
    <DebugSymbols>true</DebugSymbols>
    <DebugType>full</DebugType>
    <NoWarn>$(NoWarn);CS1591</NoWarn>
    <NoError>$(NoError);CS1591</NoError>
    <UseWPF>true</UseWPF>
  </PropertyGroup>
  <!-- Add the references for all projects and targets -->
  <ItemGroup>
    <PackageReference Include="JetBrains.Annotations" PrivateAssets="All" Version="2018.*"/>
  </ItemGroup>
</Project>